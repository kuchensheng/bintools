package bweditpost

import (
	"github.com/gin-gonic/gin"
	"github.com/rs/zerolog/log"
	"github.com/kuchensheng/bintools/json/model"
)

var tenantId = func() string{
    t := "default"
    
    return t
}()
func Executorbweditpost(ctx *gin.Context) (any, error) {
	log.Info().Msgf("请求地址:%s",ctx.Request.URL.Path)
	tId := ctx.getHeader("isc-tenant-id")
	if (tenantId != tId) {
	    return nil,model.NewBusinessException(1080403, "禁止操作：租户不匹配")
	}
	return "我是库陈胜",nil
}
